{
  "name": "conta-rapida",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:logs": "node scripts/dev-with-logs.js",
    "build": "next build",
    "start": "node server.mjs",
    "start:next": "next start",
    "build:safe": "next build --no-lint",
    "start:safe": "NODE_OPTIONS='-r next/dist/server/require-hook' next start",
    "dev:prod": "NODE_ENV=development next dev",
    "build:dynamic": "NEXT_DISABLE_STATIC_GENERATION=true next build",
    "build:skip-errors": "NEXT_DISABLE_STATIC_GENERATION=true node scripts/build-skip-errors.js",
    "lint": "next lint",
    "prepare": "husky && prisma generate",
    "postinstall": "prisma generate",
    "seed:system": "node prisma/seed-system.js",
    "seed:demo": "node prisma/seed-demo.js",
    "check:certs": "node scripts/check-certs.js",
    "test:storage": "ts-node scripts/test-storage.ts",
    "dev:typecheck": "tsc --noEmit && next dev",
    "cron:gestao-click": "curl -X GET \"http://localhost:3000/api/cron/gestao-click-sync\" -H \"Authorization: Bearer $CRON_API_KEY\"",
    "cron:schedules": "curl -X GET \"http://localhost:3000/api/cron/process-schedules\" -H \"Authorization: Bearer $CRON_API_KEY\"",
    "import:gestao-click": "ts-node scripts/auto-import-gestao-click.ts",
    "import:gestao-click-api": "curl -X GET \"http://localhost:3000/api/gestao-click/auto-import-all\"",
    "cleanup-duplicates": "ts-node --esm --transpile-only scripts/cleanup-duplicates.ts",
    "cleanup-js": "node scripts/cleanup-duplicates.js",
    "delete-transactions": "node scripts/delete-all-transactions.js",
    "toggle-demo": "node scripts/toggle-demo-mode.js",
    "dev:demo": "node scripts/run-demo.js",
    "docs:update": "node scripts/update-docs-index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:skip-integration": "jest --testPathIgnorePatterns=tests/integration",
    "test:e2e": "playwright test"
  },
  "prisma": {
    "seed": "node prisma/seed-system.js"
  },
